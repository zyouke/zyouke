<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	   xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-4.1.xsd">
	<context:property-placeholder location="classpath:dubbo.properties"/>
	<!-- 消费方应用名，用于计算依赖关系，不是匹配条件，不要与提供方一样 -->
	<dubbo:application name="${dubbo_consumer_name}" />
	<!-- 使用zookeeper注册中心暴露服务地址 -->
	<!-- 注册中心地址 -->
	<dubbo:registry protocol="zookeeper" address="${zookeeper_addr}" file="./files/dubbo/dubbo.cache"/>
	<dubbo:reference id="dubboService" interface="com.zyouke.service.IDubboService"/>
	<dubbo:reference id="dubboService20880" interface="com.zyouke.service.IDubboService" url="dubbo://122.114.90.68:20880"/>
	<dubbo:reference id="dubboService20881" interface="com.zyouke.service.IDubboService" url="dubbo://122.114.90.68:20881"/>

</beans>